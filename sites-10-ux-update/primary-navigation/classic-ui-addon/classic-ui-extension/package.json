{
    "name": "classic-ui-extension",
    "version": "1.0.0",
    "description": "Adds an iframe with the Classic UI Publishing Queue",
    "author": "RWS",
    "homepage": "https://www.rws.com",
    "license": "Apache-2.0",
    "type": "module",
    "main": "../dist/classic-ui-extension/main.js",
    "files": [
        "../dist/classic-ui-extension/*"
    ],
    "private": true,
    "scripts": {
        "build": "webpack --config ./webpack.prod.config.js --progress",
        "dev": "webpack serve --config ./webpack.dev.config.js --progress --env target=http://0.0.0.0:8080 manifest=../manifest.json",
        "pack": "sites-extensions pack --manifest=../manifest.json --input=../dist --output=../",
        "prepack": "npm run build",
        "lint": "eslint . --fix && prettier --write \"src/**/*\""
    },
    "peerDependencies": {
        "@tridion-sites/extensions": "2.0.0",
        "@tridion-sites/models": "1.1.0",
        "@tridion-sites/open-api-client": "3.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "styled-components": "5.3.6",
        "tinymce": "6.7.1"
    },
    "devDependencies": {
        "@babel/core": "7.24.5",
        "@babel/plugin-transform-runtime": "7.24.3",
        "@babel/preset-env": "7.24.5",
        "@babel/preset-react": "7.24.1",
        "@babel/preset-typescript": "7.24.1",
        "@babel/runtime": "7.24.5",
        "@tridion-sites/extensions": "2.0.0",
        "@tridion-sites/extensions-cli": "1.1.0",
        "@tridion-sites/models": "1.1.0",
        "@tridion-sites/open-api-client": "3.0.0",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/react-is": "18.2.0",
        "@types/styled-components": "5.1.26",
        "@typescript-eslint/eslint-plugin": "5.59.5",
        "@typescript-eslint/parser": "5.59.5",
        "babel-loader": "9.1.2",
        "babel-plugin-styled-components": "2.1.1",
        "eslint": "8.40.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-formatter-pretty": "5.0.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "9.0.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "fork-ts-checker-webpack-plugin": "8.0.0",
