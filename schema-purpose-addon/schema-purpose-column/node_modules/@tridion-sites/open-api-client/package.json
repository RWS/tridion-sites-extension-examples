{
  "name": "@tridion-sites/open-api-client",
  "version": "1.0.5",
  "description": "Tridion Sites Open API Client",
  "author": "RWS",
  "homepage": "https://www.rws.com",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://stash.sdl.com/scm/tdx/tridion-sites.git"
  },
  "bugs": {
    "url": "https://jira.sdl.com/projects/DXUI/issues/"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c && yarn run docs:extract --local && rimraf dist/esm/dts",
    "build:ci": "rollup -c && yarn run docs:extract && rimraf dist/esm/dts",
    "lint": "eslint . --fix && prettier --write \"src/**/*\"",
    "generate": "openapi --input src/spec.json --output src/client --client axios",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "axios": "1.2.4",
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "eslint": "8.32.0",
    "openapi-typescript-codegen": "0.23.0",
    "rimraf": "4.1.2",
    "rollup": "3.11.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-node-externals": "5.1.0",
    "typescript": "5.0.4"
  }
}