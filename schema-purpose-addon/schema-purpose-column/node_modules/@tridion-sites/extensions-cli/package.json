    "@types/fs-extra": "11.0.1",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.11.18",
    "@web/rollup-plugin-copy": "0.3.0",
    "rimraf": "4.1.2",
    "rollup": "3.11.0",
    "rollup-plugin-delete": "2.0.0",
