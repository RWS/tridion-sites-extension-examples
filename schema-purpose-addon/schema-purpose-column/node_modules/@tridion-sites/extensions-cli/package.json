    "archiver": "5.3.1",
    "chalk": "5.2.0",
    "commander": "10.0.0",
    "cross-spawn": "7.0.3",
    "decamelize": "6.0.0",
    "fs-extra": "11.1.0",
    "handlebars": "4.7.7",
    "inquirer": "9.1.4",
    "valid-filename": "4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.0.1",
