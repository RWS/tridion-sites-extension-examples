{
  "name": "@tridion-sites/models",
  "version": "0.1.1",
  "description": "Tridion Sites models",
  "author": "RWS",
  "homepage": "https://www.rws.com",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://stash.sdl.com/scm/tdx/tridion-sites.git"
  },
  "bugs": {
    "url": "https://jira.sdl.com/projects/DXUI/issues/"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c && yarn run docs:extract --local && rimraf dist/dts",
    "build:ci": "rollup -c && yarn run docs:extract && rimraf dist/dts",
    "dev": "rollup -c -w",
    "lint": "eslint . --fix && prettier --write \"src/**/*\"",
    "prepublish": "yarn build",
    "test": "jest --verbose --watch",
    "test:once": "jest --verbose",
    "test:ci": "jest --verbose --reporters='default' --reporters='jest-junit'"
  },
  "dependencies": {
    "@tridion-sites/open-api-client": "1.0.5",
    "immer": "9.0.16"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@tridion-sites/utils": "0.0.0",
    "@tridion/graphene": "1.30.1",
    "@types/jest": "29.4.0",
    "babel-jest": "29.4.0",
    "jest": "29.4.0",
    "jest-junit": "15.0.0",
    "rimraf": "4.1.2",
    "rollup": "3.11.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-node-externals": "5.1.0",
    "typescript": "5.0.4",
    "typescript-transform-paths": "3.4.6"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "junit.xml"
  }
}